#include <iostream>
#include<algorithm>
#include <cmath>
#include <array>
#include <stdio.h>
#include <string>
#include <cctype>
 
using namespace std;
 
int main() {
 
    long double a, b, c, d, sum1, sum2;
 
    cin >> a >> b >> c >> d;
 
    sum1 = b * log(a);
    sum2 = d * log(c);
 
    if(sum1 > sum2){
        cout << "YES";
    }
    else{
        cout << "NO";
    }
 
    return 0;
}
