#include <iostream>
#include <cmath>
#include <array>
#include <stdio.h>
#include <string>
 
using namespace std;
int main()
{
    string x;
    cin >> x;
    
    if (x >= "A" && x <= "z"){
        if (x >= "A" && x <= "Z"){
        cout << "ALPHA\nIS CAPITAL";
        }
        else if (x >= "a" && x <= "z"){
        cout <<"ALPHA\nIS SMALL";
        }
    }
    else{
        cout << "IS DIGIT";
    }
    
    return 0;
}
