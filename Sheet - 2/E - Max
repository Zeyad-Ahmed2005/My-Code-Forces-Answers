#include <iostream>
#include <algorithm>
#include <cmath>
#include <array>
#include <string>
#include <cctype>
 
using namespace std;
int main() {
    int array_size;
    cin >> array_size;
    int list[array_size];
    
    for (int i = 0; i < array_size; i++) {
        cin >> list[i];
    }

    sort(list, list + array_size);
    cout << list[array_size-1] << endl;

    return 0;
}
