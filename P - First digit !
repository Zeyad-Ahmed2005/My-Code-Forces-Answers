#include <iostream>
#include <cmath>
#include <array>
#include <stdio.h>
#include <string>
#include <cctype>
 
using namespace std;
int main()
{
    long long x, y, z=10;
    cin >> x;
    y = log10(x);
    
    if (x < 10){
        if (x%2 == 0){
            cout << "EVEN";
        }
        else{
            cout << "ODD";
        }
        
    }
    else{
        z = pow(z,y);
        x = x / z;
        if (x%2 == 0){
            cout << "EVEN";
        }
        else{
            cout << "ODD";
        }
    }
    return 0;
}
